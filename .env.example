# Server Configuration
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:5173

# JWT Secret
# Generate with: openssl rand -base64 32
JWT_SECRET=your_jwt_secret_here

# Google API Configuration
# Get your credentials from Google Cloud Console (https://console.cloud.google.com/)
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:3001/api/auth/google/callback

# Spreadsheet IDs
LOGIN_SPREADSHEET_ID=your_login_spreadsheet_id_here
QUESTION_BANK_SPREADSHEET_ID=your_question_bank_spreadsheet_id_here
PRACTICAL_TASKS_SPREADSHEET_ID=your_practical_tasks_spreadsheet_id_here
WORK_SUMMARY_SPREADSHEET_ID=your_work_summary_spreadsheet_id_here
PROJECT_LISTING_SPREADSHEET_ID=your_project_listing_spreadsheet_id_here

# Document IDs
TODO_DOC_ID=your_todo_doc_id_here
CREDENTIAL_DOC_ID=your_credential_doc_id_here
WORK_SUMMARY_DOC_ID=your_work_summary_doc_id_here
PROJECT_DOC_ID=your_project_doc_id_here
